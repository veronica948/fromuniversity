#include<iostream.h>
int main()
{
int n,t,x,a;
cout<<"enter number"<<endl;
cin>>n;
for(;n!=0;)
{       x=n%10;
        n=n/10;
        a=n;
        while(a)
        {t=a%10;
        if(t==x)
        {
        cout<<"yes"<<" "<<t<<endl;
        break;
        }
        a=a/10;
        }
        if(!a) cout<<"No"<<endl;
}
system("pause");
return 0;
}
