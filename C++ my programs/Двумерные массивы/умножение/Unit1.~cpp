#include<iostream.h>
#include<stdlib.h>
int main()
{ int const N=5,M=3,K=4;
  int a[N][M],b[M][K],c[N][K];
  for(int i=0;i<N;i++)
   {for(int j=0;j<N;j++)
    {a[i][j]=random(100);
    cout.width(4);
    cout<<a[i][j];
    }
    cout<<endl;
   }
   cout<<endl;

  for(int i=0;i<N;i++)
   {for(int j=0;j<N;j++)
    {b[i][j]=random(100);
    cout.width(4);
    cout<<b[i][j];
    }
    cout<<endl;
   }
   cout<<endl;
   









   system("pause");
   return 0;
}
